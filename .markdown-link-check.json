{
  "ignorePatterns": [
    {
      "pattern": "^https://img.shields.io"
    },
    {
      "pattern": "^https://registry.terraform.io"
    },
    {
      "pattern": "^https://blog.github.com"
    },
    {
      "pattern": "^https://github.com/.*/discussions"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "application/vnd.github.v3+json"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "aliveStatusCodes": [200, 206, 301, 302, 308]
}
